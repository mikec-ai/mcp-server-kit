{
	"name": "{{PROJECT_NAME}}",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat"],
	// <mcp-auth:vars>
	// Auth environment variables will be added here by add-auth command
	// </mcp-auth:vars>
	"observability": {
		"enabled": true
	},
	"durable_objects": {
		"bindings": [
			{
				"name": "MCP_OBJECT",
				"class_name": "MCPServerAgent"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": ["MCPServerAgent"]
		}
	],
	"dev": {
		"port": {{PORT}}
	}
}
